# Exemplo de variáveis de ambiente para o worker
# Copie este arquivo para .env e configure com seus valores

# Redis (Upstash) - OBRIGATÓRIO
UPSTASH_REDIS_HOST=your-redis-host.upstash.io
UPSTASH_REDIS_PORT=6379
UPSTASH_REDIS_PASSWORD=your-redis-password

# Database (PostgreSQL) - OBRIGATÓRIO
DATABASE_URL=postgresql://user:password@host:port/database?schema=public
DIRECT_URL=postgresql://user:password@host:port/database?schema=public

# Nota Paraná (opcional)
NOTA_PARANA_BASE_URL=https://menorpreco.notaparana.pr.gov.br/api/v1
LOCAL_PADRAO=Curitiba
PERIODO_PADRAO=30
RAIO_PADRAO=5000

# Railway (automático)
PORT=3000
NODE_ENV=production
